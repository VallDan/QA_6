let num1 = 2;
let num2 = 5;
let operator = "*";

if (operator == "+"){
    console.log(num1 + num2)
}
else if (operator == "-"){
    console.log(num1 - num2) 
}
else if (operator == "*"){
    console.log(num1 * num2)
}
else if (operator == "/" && num2 != 0){
    console.log(num1 / num2)
}
else console.log('Please, inter correct number')